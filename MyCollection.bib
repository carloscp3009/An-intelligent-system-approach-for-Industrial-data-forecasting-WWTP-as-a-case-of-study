issn = {10010742},
journal = {Journal of Environmental Sciences (China)},
keywords = {Artificial neural network,Effluent concentration,Prediction accuracy,Sensitivity analysis,Support vector machine},
pages = {90--101},
publisher = {Elsevier B.V.},
title = {{Prediction of effluent concentration in a wastewater treatment plant using machine learning models}},
url = {http://dx.doi.org/10.1016/j.jes.2015.01.007},
volume = {32},
