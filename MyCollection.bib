issn = {13648152},
journal = {Environmental Modelling and Software},
keywords = {Process monitoring and supervision,Soft-sensors,Switching system,Wastewater treatment},
pages = {215--229},
publisher = {Elsevier Ltd},
title = {{Shall we use hardware sensor measurements or soft-sensor estimates? Case study in a full-scale WWTP}},
