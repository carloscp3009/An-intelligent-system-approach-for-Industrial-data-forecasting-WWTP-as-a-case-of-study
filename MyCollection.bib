issn = {13091042},
journal = {Atmospheric Pollution Research},
keywords = {Artificial intelligence,Environmental engineering,Hydraulics,Hydrogen sulfiide,Soft computing},
number = {6},
pages = {1812--1822},
publisher = {Elsevier B.V.},
title = {{Multivariate NARX neural network in prediction gaseous emissions within the influent chamber of wastewater treatment plants}},
