journal = {Economic and Social Development: Book of Proceedings},
keywords = {19th century,Algorithms,Artificial intelligence,Automation,Big Data,Civil law,Collaboration,Computer information security,Computers,Cooperation,Data analysis,Data management,Data processing,Employment,Exploitation,Industrial Revolution,Industrial applications,Industrial development,Information technology,Laws,Mass production,Mechanization,New technology,Privacy,Production methods,Raw materials,Robotics,Robots,Security,Sociology,Steam,Steam electric power generation,Workers},
number = {November},
pages = {742--745},
title = {{Industry 4.0 : The Exploitation of Big Data and Forthcoming Perspectives}},
