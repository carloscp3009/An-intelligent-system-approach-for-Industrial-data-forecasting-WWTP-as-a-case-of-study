issn = {18791026},
keywords = {Artificial neural network,Early-warning,Environmental pollutants,Intelligent control,Soft measurement},
pages = {134279},
publisher = {Elsevier B.V},
title = {{Tackling environmental challenges in pollution controls using artificial intelligence: A review}},
url = {https://doi.org/10.1016/j.scitotenv.2019.134279},
volume = {699},
